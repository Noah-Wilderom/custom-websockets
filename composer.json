{
    "name": "noahwilderom/custom-websockets",
    "description": "",
    "keywords": [
        "noahwilderom",
        "laravel",
        "custom-websockets"
    ],
    "homepage": "https://github.com/Noah-Wilderom/custom-websockets",
    "license": "MIT",
    "authors": [
        {
            "name": "Noah Wilderom",
            "email": "wilderomnoah@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "cboden/ratchet": "^0.4.4",
        "illuminate/contracts": "^10.0",
		"react/socket": "^1.14",
		"react/event-loop": "^1.4",
		"ratchet/pawl": "^0.4.1"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.8",
        "nunomaduro/larastan": "^2.0.1",
        "pestphp/pest": "^2.20",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^10"
    },
    "autoload": {
        "psr-4": {
            "NoahWilderom\\CustomWebsockets\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NoahWilderom\\CustomWebsockets\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NoahWilderom\\CustomWebsockets\\CustomWebsocketsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
